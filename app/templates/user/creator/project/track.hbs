{{!-- 
  this template for the /track/:id route is rendered as the footer for the currently selected track
 --}} 
<TrackFooterContainer
  @track={{@model}}
  @toggleSidebar={{fn this.toggleSidebar @track.id}}
  @rightSidebarOpen={{this.project.rightSidebarOpen}}
  @leftSidebarOpen={{this.project.leftSidebarOpen}}
/>

{{outlet}}